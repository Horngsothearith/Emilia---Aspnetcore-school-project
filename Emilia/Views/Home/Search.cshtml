@{
    ViewData["Title"] = "Search Result";
}


<hr>
<div class="row">
    <div class="col-sm-9">
        <div class="row">
                @for(var i = 0; i<24; i++)
                {
                <div class="col-sm-3  mt-1 p-0">
                    <div class="card item">
                        <img class="card-img-top" src="~/images/002.jpg" alt="pre-img">
                        <div class="card-body">
                            <h5 class="card-title">$4.99</h5>
                            <p class="card-text">Original custom from japan. Made from 100% cottons</p>
                            <a class="btn btn-primary" href="#">Order</a>
                        </div>
                    </div>  
                </div>
                }
        </div>
            <nav style="display:inline-block;" class="mt-2" aria-label="Page navigation example">
            <ul class="pagination">
                <li class="page-item disabled">
                    <a class="page-link" href="#" tabindex="-1">Previous</a>
                 </li>
                @for(var i = 0; i<10; i++)
                {
                    <li class="page-item"><a class="page-link" href="#">@i</a></li>
                }
                <li class="page-item">
                <a class="page-link" href="#">Next</a>
                </li>
                <li class="ml-3">
                    <form method="GET" class=" form-inline">
                            <div class="form-group">
                                <input class="form-control" name="page" type="number|text" placeholder="Go to page">
                              <input class="btn ml-1" type="submit" value="GO">
                        </div>
                    </form>
                </li>
            </ul>
            </nav>
           
    </div>
    <div class="col-sm-3">
        @await Html.PartialAsync("_SearchPatial")
    </div>
</div>

<div>

</div>